<nav class="navbar navbar-expand-lg navbar-dark bg-success">
  <a class="navbar-brand" href="/">Kotaku Scraper</a>
  <button class="navbar-toggler" data-toggle="collapse" data-target="#">
    <span class="navbar-toggler-icon"></span>
  </button>
</nav>
<div id="container text-center" style="margin-left:5%">
      <div class="row">
        <div class="col-xs-12">
          <h1><strong>Scrape Kotaku</strong></h1>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12">
          <button id="scrape-button">Scrape for Articles</button>
          <a href="/saved"><button style="margin-left:2%;">Saved Articles</button></a>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-6"></div>     
        <div class="col-xs-6">
            <div id="logo" style="margin-left: 200px; margin-top: 30px;"></div>
        </div>
      </div>
    </div>

    <div class="fluid-container">
      <div id="wrapper">
        <h5 class="page-title text-center">Scraped Articles</h5>
        <div id="articles" class="row m-2">
          {{#each articles}}
          {{>general-view}}
          {{/each}}
        </div>
      </div>
    </div>
    <footer class="footer">
      <div class="footer-container">
        <p>Kotaku Games</p>
      </div>
    </footer>